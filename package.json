{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:next\" \"pnpm dev:app\" \"pnpm json-server --watch constants/db.json --port 3001\"",
    "dev:next": "next dev --turbopack",
    "dev:app": "pnpm build:electron && pnpm dev:electron",
    "dev:electron": "electron dist/main.mjs",
    "build": "next build",
    "build:electron": "vite build -c vite.config.electron.ts",
    "vite": "vite",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.67.2",
    "@tanstack/react-query-devtools": "^5.67.2",
    "@tanstack/react-query-next-experimental": "^5.67.2",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "lodash": "^4.17.21",
    "next": "15.2.1",
    "next-auth": "5.0.0-beta.25",
    "path-to-regexp": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "theme-change": "^2.5.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.0.0",
    "electron": "^35.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "json-server": "1.0.0-beta.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1"
  }
}